# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /kellyslater                controllers.Application.kellyslater()
GET     /maliamanuel                controllers.Application.maliamanuel()
GET     /jakemarshall               controllers.Application.jakemarshall()
GET     /eddie                      controllers.Application.eddie()
GET     /joyce                      controllers.Application.joycehoffman()
GET     /daize                      controllers.Application.daize()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
